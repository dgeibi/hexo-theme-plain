{
  "name": "hexo-theme-plain",
  "version": "4.0.0",
  "description": "a plain hexo theme",
  "private": true,
  "scripts": {
    "fix": "eslint . --fix",
    "build": "webpack",
    "watch": "webpack --watch",
    "precommit": "lint-staged",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,md,scss}\" \"**/.*rc\""
  },
  "browserslist": [
    "> 3% in CN",
    "ie 10",
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgeibi/hexo-theme-plain.git"
  },
  "keywords": [
    "hexo",
    "theme"
  ],
  "author": "dgeibi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dgeibi/hexo-theme-plain/issues"
  },
  "homepage": "https://github.com/dgeibi/hexo-theme-plain#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "autoprefixer": "^8.0.0",
    "babel-loader": "^8.0.0-beta.0",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.0",
    "eslint-config-dgeibi": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.0",
    "prettier": "^1.10.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "webpack": "^3.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "*.{md,scss}": [
      "prettier --write",
      "git add"
    ],
    ".*rc": [
      "prettier --write",
      "git add"
    ]
  }
}
